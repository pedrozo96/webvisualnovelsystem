<!DOCTYPE html>
<html>
<head>
  <title>WebVisualNovelSystem</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%=javascript_include_tag :webgl%>
  <%= csrf_meta_tags %>
  <%= stylesheet_link_tag 'application', 'https://fonts.googleapis.com/css?family=Dancing+Script|Raleway' %>

</head>
<body>

  <div class="wrapper">
    <header class="main-head">
      <div id="main_content">
        <%= render 'layouts/header' %>
      </div>
    </header>
    <nav class="main-nav">
      <%= render 'layouts/sidebar' %>
    </nav>
    <article class="content">
      <%= yield %>
    </article> 
    <footer class="main-footer">
      <div class="footer">
        <br><br><br><br>Footer<br><br><br><br><br>
      </div>
    </footer>
  </div>

</body>
</html>
